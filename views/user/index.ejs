<div class="panel panel-default">
  <div class="panel-body">
	<h1>Lista de Usuarios</h1>
	<a href="/user/create">Nuevo Usuario</a>
	<table class="table">
		<thead>
			<th>E-mail</th>
			<th>Nombre</th>
			<th>Apellidos</th>
			<th>Edad</th>
			<th>Fecha de nacimiento</th>
			<th>GÃ©nero</th>
		</thead>
		<% for (var i=0; i<registros.length; i++){ %>
			<tr>
				<td><%= registros[i].email %></td>
				<td><%= registros[i].nombre %></td>
				<td><%= registros[i].apellidos %></td>
				<td><%= registros[i].edad %></td>
				<td><%= registros[i].fecha_nacimiento %></td>
				<td><%= generos[registros[i].genero] %></td>

			</tr>
		<% } %>
	</table>
  </div>
</div>